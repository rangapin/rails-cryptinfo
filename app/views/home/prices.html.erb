

<% if @symbol %>
  <% if @quote["Response"] %>
      <h4>This symbol doesn't exist</h4>

 <% else %>
    <% @quote["DISPLAY"].each do |key, value| %>
    <div class="jumbotron">
      <h1 class="display-4"></h1>
      <%= key %> - <%= value["USD"]["PRICE"] %><br/>
      24H High <%= value["USD"]["HIGHDAY"] %><br/>
      24H Low <%= value["USD"]["LOWDAY"] %><br/>
      Market Cap <%= value["USD"]["MKTCAP"] %><br/>
    </div>
  <% end %>

<% end %>
    <% else %>
        <h4>Please enter a crypto symbol</h4>
<% end %>


